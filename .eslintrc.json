{
  "env": {
    "commonjs": true,
    "es2021": true,
    "node": true
  },
  "extends": "airbnb-base",
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "rules": {
    "max-len": "off",
    "no-console": "off",
    "no-plusplus": "off",
    "import/order": "off",
    "object-curly-newline": "off"
  }
}
